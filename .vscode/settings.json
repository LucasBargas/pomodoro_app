{
  "code-runner.executorMap": {
    "typescript": "npx ts-node --files --transpile-only",
  },
  "liveServer.settings.port": 5501,
  "files.exclude": {
    "node_modules": true,
    ".vscode": true,
    "public": false,
    ".git": true,
    ".gitignore": true,
    "package.json": false,
    "package-lock.json": true,
    ".eslintrc.js": false,
    ".editorconfig": true,
    ".prettierrc.js": true,
    "tsconfig.json": true,
    "README.md": true,
  }
}
